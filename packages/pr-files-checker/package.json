{
  "name": "gh-pr-files-checker",
  "version": "0.0.0",
  "private": true,
  "description": "Action that checks if the updated files follow the recommendation from the Bonita documentation team",
  "main": "lib/src/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "package:all": "npm run format && npm run build && npm run package",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bonitasoft/actions.git"
  },
  "keywords": [
    "github",
    "pr-checker"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bonitasoft/actions/issues"
  },
  "homepage": "https://github.com/bonitasoft/actions/tree/main/packages/pr-files-checker#readme",
  "dependencies": {
    "@actions/core": "^1.10.0"
  },
  "devDependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.0.0",
    "@types/node": "^18.16.3",
    "@typescript-eslint/parser": "^5.59.2",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.39.0",
    "eslint-plugin-github": "^4.10.1",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
